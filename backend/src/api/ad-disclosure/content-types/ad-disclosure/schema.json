{
  "attributes": {
    "adElection": {
      "enum": [
        "Election 1",
        "Election 2",
        "Election 3"
      ],
      "required": true,
      "type": "enumeration"
    },
    "adFormat": {
      "enum": [
        "Digital",
        "Print",
        "Direct Mail",
        "Tv",
        "Radio"
      ],
      "required": true,
      "type": "enumeration"
    },
    "adMedia": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "multiple": true,
      "required": true,
      "type": "media"
    },
    "adSpend": {
      "required": true,
      "type": "decimal"
    },
    "adTextContent": {
      "type": "text"
    },
    "authorizedAdSpend": {
      "type": "decimal"
    },
    "candidatesMeasuresAndPoliticalParties": {
      "components": [
        "ad-disclosure.candidate",
        "ad-disclosure.measure",
        "ad-disclosure.political-party"
      ],
      "required": true,
      "type": "dynamiczone"
    },
    "clickCount": {
      "type": "integer"
    },
    "endDate": {
      "required": true,
      "type": "date"
    },
    "externalLink": {
      "regex": "^(http(s):\\/\\/.)[-a-zA-Z0-9@:%._\\+~#=]{2,256}\\.[a-z]{2,6}\\b([-a-zA-Z0-9@:%_\\+.~#?&//=]*)$",
      "type": "string"
    },
    "startDate": {
      "required": true,
      "type": "date"
    },
    "targetAudience": {
      "type": "text"
    },
    "viewCount": {
      "type": "integer"
    }
  },
  "collectionName": "ad_disclosures",
  "info": {
    "description": "",
    "displayName": "Ad Disclosure",
    "pluralName": "ad-disclosures",
    "singularName": "ad-disclosure"
  },
  "kind": "collectionType",
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {}
}
